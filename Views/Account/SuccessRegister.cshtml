@model Spotify.Models.DTOs.RegisterDto
@{
    ViewData["Title"] = $"{Model.Email} - Register Successfull";
}

<h1>Register has been successfully completed</h1>
<div class="alert alert-success">
    Confirmation email sent to @Model.Email
    <p>
        <a asp-controller="Account" asp-action="Login">Login to App</a>
    </p>
</div>